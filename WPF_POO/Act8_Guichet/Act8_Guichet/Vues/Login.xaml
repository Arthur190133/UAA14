<Page x:Class="Act8_Guichet.Vues.Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Act8_Guichet.Vues"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Login">

    <Grid>
        <WrapPanel Height="450" Width="800" Background="Gray">
            <TextBox x:Name="LoginId"></TextBox>
            <TextBox x:Name="LoginPassword">Password</TextBox>
            <Button Click="LoginButton">Retirer Fond</Button>
        </WrapPanel>

    </Grid>
</Page>
